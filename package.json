{
  "name": "www.foodie.com--server",
  "version": "1.0.0",
  "description": "This is a Node/typescript server. A server for ecommerce based online food ordering site, with functionality of Real Time Order Status using Socket.io",
  "main": "src/Server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/Server.js",
    "dev": "nodemon -r esm ./src/Server.ts",
    "build":"tsc --project .",
    "dev-ts-node": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/index.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sunilgawai/www.foodie.com--server.git"
  },
  "author": "Sunil Gawai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunilgawai/www.foodie.com--server/issues"
  },
  "homepage": "https://github.com/sunilgawai/www.foodie.com--server#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/esm": "^3.2.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.14",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
